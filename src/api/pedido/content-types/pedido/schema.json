{
  "kind": "collectionType",
  "collectionName": "pedidos",
  "info": {
    "singularName": "pedido",
    "pluralName": "pedidos",
    "displayName": "Pedido"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Usuario": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "FechaPedido": {
      "type": "datetime",
      "required": true
    },
    "Estado": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Pendiente",
        "Retirado con exito"
      ]
    },
    "Total": {
      "type": "decimal",
      "required": true
    }
  }
}
