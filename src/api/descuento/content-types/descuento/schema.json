{
  "kind": "collectionType",
  "collectionName": "descuentos",
  "info": {
    "singularName": "descuento",
    "pluralName": "descuentos",
    "displayName": "Descuento"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Titulo": {
      "type": "string",
      "required": true
    },
    "Descripcion": {
      "type": "text",
      "required": true
    },
    "Imagen": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "Vigencia_Desde": {
      "type": "date",
      "required": true
    },
    "Vigencia_Hasta": {
      "type": "date"
    },
    "Estado": {
      "type": "boolean",
      "required": true
    },
    "producto": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::producto.producto"
    }
  }
}
