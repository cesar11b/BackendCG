{
  "kind": "collectionType",
  "collectionName": "reservas",
  "info": {
    "singularName": "reserva",
    "pluralName": "reservas",
    "displayName": "Reserva"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Fecha": {
      "type": "datetime",
      "required": true
    },
    "Estado": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Activo",
        "Inactivo"
      ]
    },
    "Asistencia": {
      "type": "boolean",
      "required": true
    },
    "Observaciones": {
      "type": "text"
    },
    "Usuario": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "servicio": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::servicio.servicio"
    }
  }
}
